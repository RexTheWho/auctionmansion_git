<?xml version="1.0"?>
<dynamic_object>
	<diesel materials="units/pd2_mod_auction/mansion/auction_mansion" orientation_object="rp_rootpoint" />
	
	<bodies>
		<body name="body_static" enabled="true" template="static">
			<object name="c_box_001"		collision_type="box"		padding="-2.5" />
			<object name="c_box_002"		collision_type="box"		padding="-2.5" />
			<object name="c_box_003"		collision_type="box"		padding="-2.5" />
			<object name="c_box_004"		collision_type="box"		padding="-2.5" />
			<object name="c_capsule_001"	collision_type="capsule"	padding="-2.5" />
		</body>
	</bodies>
	
	<decal_surfaces default_material="plaster">
		<decal_mesh name="c_box_001" enabled="true" material="grass" />
		<decal_mesh name="c_box_002" enabled="true" material="grass" />
		<decal_mesh name="c_box_003" enabled="true" material="grass" />
		<decal_mesh name="c_box_004" enabled="true" material="grass" />
		<decal_mesh name="c_capsule_001" enabled="true" material="grass" />
	</decal_surfaces>
	
	<graphics>
		<graphic_group name="g_group" enabled="true" culling_object="co_culling">
			<object name="g_mortar"			enabled="true" shadow_caster="true"/>
			<object name="g_mortar_pillar"	enabled="true" shadow_caster="true"/>
			<object name="g_mortar_trims"	enabled="true" shadow_caster="true"/>
		</graphic_group>
	</graphics>
</dynamic_object>