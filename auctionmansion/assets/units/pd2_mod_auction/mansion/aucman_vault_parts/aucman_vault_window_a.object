<?xml version="1.0"?>
<dynamic_object>
	<diesel materials="units/pd2_mod_auction/mansion/aucman_vault_parts/aucman_vault_parts" orientation_object="rp_rootpoint" />
	<sequence_manager file="units/pd2_mod_auction/mansion/aucman_vault_parts/aucman_vault_window"/>
	
	<bodies>
		<body name="body_static" enabled="true" template="static">
			<object name="c_box_001" collision_type="box" padding="-2.5" />
			<object name="c_box_002" collision_type="box" padding="-2.5" />
			<object name="c_box_003" collision_type="box" padding="-2.5" />
			<object name="c_box_004" collision_type="box" padding="-2.5" />
			<object name="c_box_005" collision_type="box" padding="-2.5" />
		</body>
		<body name="body_glass" enabled="true" template="window_bulletproof">
			<object name="c_glass" collision_type="box" padding="-2" />
		</body>
	</bodies>
	
	<decal_surfaces default_material="steel">
		<decal_mesh name="c_box_001"	enabled="true" material="steel" />
		<decal_mesh name="c_box_002"	enabled="true" material="steel" />
		<decal_mesh name="c_box_003"	enabled="true" material="steel" />
		<decal_mesh name="c_box_004"	enabled="true" material="steel" />
		<decal_mesh name="c_box_005"	enabled="true" material="steel" />
		<decal_mesh name="g_glass"		enabled="true" material="glass_unbreakable" />
	</decal_surfaces>
	
	
	<graphics>
		<graphic_group name="g_group" enabled="true" culling_object="co_culling">
			<object name="g_g"				enabled="true" shadow_caster="true"/>
			<object name="g_glass"			enabled="true" />
			<object name="g_glass_crk"		enabled="false" />
			<object name="g_glass_sht"		enabled="false" />
		</graphic_group>
	</graphics>
</dynamic_object>