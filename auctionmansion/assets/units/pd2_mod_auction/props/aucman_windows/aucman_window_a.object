<dynamic_object>
	<diesel materials="units/pd2_mod_auction/props/aucman_windows/aucman_windows" orientation_object="rp_rootpoint" />
	<sequence_manager file="units/pd2_mod_auction/props/aucman_windows/aucman_window_a"/>
	
	<bodies>
		<body name="body_static" enabled="true" template="static" >
			<object name="c_box_001" collision_type="box" padding="-2.5" />						
			<object name="c_box_002" collision_type="box" padding="-2.5" />						
			<object name="c_box_003" collision_type="box" padding="-2.5" />						
			<object name="c_box_004" collision_type="box" padding="-2.5" />						
		</body>
		<body name="body_glass" enabled="true" template="window" >
			<object name="c_glass" collision_type="box" padding="-2.5" />
		</body>
	</bodies>

	<graphics>
		<graphic_group name="grp_main" enabled="true" culling_object="co_culling">
			<object name="g_frame"				enabled="true"	shadow_caster="true"/>
			
			<object name="g_beams_solid"		enabled="true"	shadow_caster="true"/>
			<object name="g_beams_broken"		enabled="false"	shadow_caster="true"/>
			
			<object name="g_glass_solid"		enabled="true" />
			<object name="g_glass_cracked"		enabled="false" />
			<object name="g_glass_shattered"	enabled="false" />
		</graphic_group>
	</graphics>
</dynamic_object>
