           	<object name="c_box_glass_02_b" collision_type="box" padding="-2.5"/>
        </body>
		<body name="glass_body_03" enabled="true" template="static" remove_ray_type="vis_graph ai_vision walk">
			<object name="e_03" />
           	<object name="c_box_glass_03" collision_type="box" padding="-2.5"/>
        </body>
		<body name="glass_body_04" enabled="true" template="static" remove_ray_type="vis_graph ai_vision walk">
			<object name="e_04" />
           	<object name="c_box_glass_04" collision_type="box" padding="-2.5"/>
        </body>
		
		<body name="air_glass_body_1" enabled="true" template="static" keyframed="true" remove_ray_type="vis_graph ai_vision walk"> 
           	<object name="a_air_window_1"/>
           	<object name="c_box_air_window_1" collision_type="box" padding="-2.5"/>
        </body>
		
    </bodies>
	
	<decal_surfaces>
