            <object collision_type="mesh_mopp" name="g_g"/>
        </body>
		
		<body name="glass_body_01" enabled="true" template="static"  remove_ray_type="vis_graph ai_vision walk" >
			<object name="e_01" />
           	<object name="c_box_glass_01_a" collision_type="box" padding="-2.5"/>
           	<object name="c_box_glass_01_b" collision_type="box" padding="-2.5"/>
        </body>
		<body name="glass_body_02" enabled="true" template="static" remove_ray_type="vis_graph ai_vision walk">
			<object name="e_02" />
