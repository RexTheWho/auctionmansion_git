        <decal_mesh enabled="true" name="dm_glass_02" material="glass_breakable" />
        <decal_mesh enabled="true" name="dm_glass_03" material="glass_breakable" />
        <decal_mesh enabled="true" name="dm_glass_04" material="glass_breakable" />
        <decal_mesh enabled="true" name="dm_air_window_glass_1" material="glass_breakable" />
    </decal_surfaces>
	
    <graphics>
        <object enabled="true" name="s_s" shadow_caster="true" />
        <object enabled="true" name="s_s_01" shadow_caster="true" />
        <object enabled="true" name="s_s_02" shadow_caster="true" />
        <object enabled="true" name="s_s_03" shadow_caster="true" />
        <object enabled="true" name="s_s_04" shadow_caster="true" />
		
        <object enabled="true" name="g_g"/>
        <object enabled="true" name="g_glass_01" />
        <object enabled="true" name="g_glass_02" />
