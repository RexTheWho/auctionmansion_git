<?xml version="1.0"?>
<dynamic_object>
	<diesel materials="units/pd2_mod_auction/mansion/auction_mansion" orientation_object="rp_rootpoint" />
	<sequence_manager file="units/pd2_mod_auction/mansion/aucman_doubledoor_caps" />
	
	<bodies>
		<body name="body_solid" enabled="true" template="static">
			<object name="c_box_000"	collision_type="box" padding="-2.5"/>
		</body>
		<body name="body_door" enabled="false" template="static">
			<object name="c_box_001"	collision_type="box" padding="-2.5"/>
			<object name="c_box_002"	collision_type="box" padding="-2.5"/>
			<object name="c_box_003"	collision_type="box" padding="-2.5"/>
			<object name="c_box_004"	collision_type="box" padding="-2.5"/>
			<object name="c_box_005"	collision_type="box" padding="-2.5"/>
		</body>
		<body name="body_exploded" enabled="false" template="static">
			<object name="c_box_006"	collision_type="box" padding="-2.5"/>
			<object name="c_box_007"	collision_type="box" padding="-2.5"/>
			<object name="c_box_008"	collision_type="box" padding="-2.5"/>
		</body>
	</bodies>
	
	<graphics>
		<graphic_group name="grp_solid" enabled="true" culling_object="co_culling">
			<object name="g_ext_solid"		enabled="true"	shadow_caster="true"/>
			<object name="g_int_solid_1"	enabled="true"	shadow_caster="true"/>
			<object name="g_int_solid_2"	enabled="false"	shadow_caster="true"/>
			<object name="g_int_solid_3"	enabled="false"	shadow_caster="true"/>
		</graphic_group>
		<graphic_group name="grp_door" enabled="false" culling_object="co_culling">
			<object name="g_ext_door"		enabled="true"	shadow_caster="true"/>
			<object name="g_int_door_1"		enabled="true"	shadow_caster="true"/>
			<object name="g_int_door_2"		enabled="false"	shadow_caster="true"/>
			<object name="g_int_door_3"		enabled="false"	shadow_caster="true"/>
		</graphic_group>
		<graphic_group name="grp_exploded" enabled="false" culling_object="co_culling">
			<object name="g_ext_ploded"			enabled="true"	shadow_caster="true"/>
			<object name="g_exploded_decals"	enabled="true"	shadow_caster="false"/>
			<object name="g_exploded_bricks"	enabled="true"	shadow_caster="false"/>
			<object name="g_int_ploded_1"		enabled="true"	shadow_caster="true"/>
			<object name="g_int_ploded_2"		enabled="false"	shadow_caster="true"/>
			<object name="g_int_ploded_3"		enabled="false"	shadow_caster="true"/>
