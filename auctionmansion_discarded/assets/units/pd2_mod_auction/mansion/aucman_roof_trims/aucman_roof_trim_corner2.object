<?xml version="1.0"?>
<dynamic_object>
	<diesel materials="units/pd2_mod_auction/mansion/auction_mansion" orientation_object="rp_rootpoint" />
	
	<bodies>
		<body name="body_static" enabled="true" template="static">
			<object name="c_c1"	collision_type="convex" padding="-2.5" />
			<object name="c_c2"	collision_type="convex" padding="-2.5" />
		</body>
	</bodies>
	
	<decal_surfaces default_material="plaster">
		<decal_mesh name="c_c1" enabled="true" material="grass" />
		<decal_mesh name="c_c2" enabled="true" material="grass" />
	</decal_surfaces>
	
	
	<graphics>
		<graphic_group name="g_group" enabled="true" culling_object="co_culling">
			<object name="g_g" enabled="true" shadow_caster="true"/>
		</graphic_group>
	</graphics>
</dynamic_object>