<?xml version="1.0"?>
<dynamic_object>
	<diesel materials="units/pd2_mod_auction/mansion/auction_mansion" orientation_object="rp_rootpoint" />
	<sequence_manager file="units/pd2_mod_auction/mansion/aucman_vault_ceiling_floor/aucman_vault_ceiling_floor" />
	
	<bodies>
		<body name="body_solid" enabled="true" template="static">
			<object name="c_box_solid"	collision_type="box" padding="-2.5"/>
		</body>
		
		<body name="body_mover" enabled="false" template="mover_blocker">
			<object name="c_moverblocker"	collision_type="mesh_mopp" />
		</body>
		
		<body name="body_exploded" enabled="false" template="static">
			<object name="c_box_001"	collision_type="box" padding="-2.5"/>
			<object name="c_box_002"	collision_type="box" padding="-2.5"/>
			<object name="c_box_003"	collision_type="box" padding="-2.5"/>
			<object name="c_box_004"	collision_type="box" padding="-2.5"/>
			<object name="c_box_013"	collision_type="box" padding="-2.5"/>
		</body>
		
		<body name="body_cut_static" enabled="false" template="static">
			<object name="c_box_001"	collision_type="box" padding="-2.5"/>
			<object name="c_box_002"	collision_type="box" padding="-2.5"/>
			<object name="c_box_003"	collision_type="box" padding="-2.5"/>
			<object name="c_box_004"	collision_type="box" padding="-2.5"/>
			<object name="c_box_005"	collision_type="box" padding="-2.5"/>
			<object name="c_box_006"	collision_type="box" padding="-2.5"/>
			<object name="c_box_007"	collision_type="box" padding="-2.5"/>
			<object name="c_box_008"	collision_type="box" padding="-2.5"/>
			<object name="c_box_009"	collision_type="box" padding="-2.5"/>
			<object name="c_box_010"	collision_type="box" padding="-2.5"/>
			<object name="c_box_011"	collision_type="box" padding="-2.5"/>
			<object name="c_box_012"	collision_type="box" padding="-2.5"/>
		</body>
		<body name="body_disk_dynamic" enabled="false" template="dynamic_not_pushable" collides_with_mover="true" >
			<object name="a_disk" />
			<object name="c_disk"		collision_type="convex" padding="-2.5" mass="500"/>
		</body>
	</bodies>
	
	<decal_surfaces default_material="wood">
		<decal_mesh name="g_solid_vault"	enabled="true"	material="metal" />
		<decal_mesh name="g_dest_vault"		enabled="true"	material="metal" />
		<decal_mesh name="g_disk"			enabled="true"	material="metal" />
	</decal_surfaces>
	
	<graphics>
		<graphic_group name="g_group" enabled="true" culling_object="co_culling">
			<object name="g_dest_decals"		enabled="false"	shadow_caster="true"/>
			<object name="g_dest_concrete"		enabled="false"	shadow_caster="true"/>
			<object name="g_dest_vault"			enabled="false"	shadow_caster="true"/>
			<object name="g_hole_glow"			enabled="false"	shadow_caster="true"/>
			<object name="g_dest_floor1"		enabled="false"	shadow_caster="true"/>
			<object name="g_dest_floor2"		enabled="false"	shadow_caster="true"/>
			<object name="g_dest_floor3"		enabled="false"	shadow_caster="true"/>
			<object name="g_dest_floor4"		enabled="false"	shadow_caster="true"/>
			<object name="g_solid_vault"		enabled="true"	shadow_caster="true"/>
			<object name="g_solid_floor1"		enabled="true"	shadow_caster="true"/>
			<object name="g_solid_floor2"		enabled="true"	shadow_caster="true"/>
			<object name="g_solid_floor3"		enabled="true"	shadow_caster="true"/>
			<object name="g_solid_floor4"		enabled="true"	shadow_caster="true"/>
		</graphic_group>
		<graphic_group name="g_group" enabled="true" culling_object="g_disk">
			<object name="g_disk"				enabled="false"	shadow_caster="true"/>
		</graphic_group>
	</graphics>
</dynamic_object>