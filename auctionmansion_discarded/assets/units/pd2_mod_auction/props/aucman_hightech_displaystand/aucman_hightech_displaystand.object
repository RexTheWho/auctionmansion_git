<?xml version="1.0"?>
<dynamic_object>
	<diesel materials="units/pd2_mod_auction/props/aucman_hightech_displaystand/aucman_hightech_displaystand" orientation_object="rp_rootpoint" />
	<sequence_manager file="units/pd2_mod_auction/props/aucman_hightech_displaystand/aucman_hightech_displaystand" />

	<bodies>
		<body name="body_static" enabled="true" template="static">
			<object name="rp_rootpoint" />
			<object name="c_box_001" collision_type="box" padding="-2.5" />
			<object name="c_box_002" collision_type="box" padding="-2.5" />
			<object name="c_box_003" collision_type="box" padding="-2.5" />
		</body>
	</bodies>

	<decal_surfaces default_material="thin_layer">
		<decal_mesh name="g_base"			enabled="true" material="wood" />
		<decal_mesh name="g_panel"			enabled="true" material="wood" />
		<decal_mesh name="g_panel_opened"	enabled="true" material="wood" />
		<decal_mesh name="g_gensec"			enabled="true" material="thin_layer" />
	</decal_surfaces>
	
	<graphics>
		<graphic_group name="grp_main" enabled="true" culling_object="co_culling">
			<object name="g_base"			enabled="true"/>
			<object name="g_mul"			enabled="true"/>
			<object name="g_gensec"			enabled="true"/>
			<object name="g_security"		enabled="true"/>
			
			<object name="g_panel"			enabled="true"/>
			<object name="g_panel_opened"	enabled="false"/>
			
			<object name="g_gui_lockpick"	enabled="true"/>
		</graphic_group>
	</graphics>
</dynamic_object>