<?xml version="1.0"?>
<dynamic_object>
    <diesel materials="units/world/props/playground/playground" orientation_object="rp_playground_swing" />
	
	<animations>
		<animation_group name="ag_swing_1" enabled="true">
			<object name="hinge_swing_1" />
			<object name="hinge_swing_2" />
			<object name="g_swing_1" />
			<object name="g_swing_2" />
			<object name="s_swing_1" />
			<object name="s_swing_2" />
		</animation_group>
	</animations>
	
	
	<bodies>
		<body name="static_body" enabled="true" template="static" >
			<object name="rp_playground_swing" />
			<object name="c_swing_1" padding="-2" collision_type="box" />
			<object name="c_swing_2" padding="-2" collision_type="box" />
			<object name="c_swing_3" padding="-2" collision_type="box" />
			<object name="c_swing_4" padding="-2" collision_type="box" />
			<object name="c_swing_5" padding="-2" collision_type="box" />
		</body>
	</bodies>

	
    <graphics>
        <object name="g_frame" enabled="true" />
        <object name="s_frame" enabled="true" shadow_caster="true" />
        <object name="g_swing_1" enabled="true" />
        <object name="g_swing_2" enabled="true" />
        <object name="s_swing_1" enabled="true" shadow_caster="true" />
        <object name="s_swing_2" enabled="true" shadow_caster="true" />
    </graphics>
	
	
	<decal_surfaces default_material="plastic" static="true" >
        <decal_mesh enabled="true" name="dm_metal" material="sheet_metal" />
    </decal_surfaces>
	
</dynamic_object> 